<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane minHeight="-Infinity" minWidth="-Infinity" prefHeight="700.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.ui.MainController">
   <top>
      <MenuBar BorderPane.alignment="CENTER">
         <menus>
            <Menu mnemonicParsing="false" text="File">
               <items>
                  <MenuItem mnemonicParsing="false" text="Close" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Edit">
               <items>
                  <MenuItem mnemonicParsing="false" text="Delete" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Help">
               <items>
                  <MenuItem mnemonicParsing="false" text="About" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
   </top>
   <center>
      <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <HBox minHeight="-Infinity" prefHeight="675.0">
               <children>
                  <FlowPane prefHeight="641.0" prefWidth="150.0">
                     <children>
                        <Accordion prefWidth="150.0">
                          <panes>
                            <TitledPane animated="false" text="查询">
                                 <content>
                                    <VBox prefHeight="100.0" prefWidth="100.0">
                                    <padding>
                                          <Insets bottom="0" left="0" right="0" top="0" />
                                       </padding>
                                       <children>
                                          <Button fx:id="buttonRepoSearch" mnemonicParsing="false" onMousePressed="#onRepoSearchClicked" prefHeight="50.0" prefWidth="150.0" text="项目查询">
                                             <font>
                                                <Font size="18.0" />
                                             </font>
                                          </Button>
                                          <Button fx:id="buttonUserSearch" mnemonicParsing="false" onMousePressed="#onUserSearchClicked" prefHeight="50.0" prefWidth="150.0" text="用户查询">
                                             <font>
                                                <Font size="18.0" />
                                             </font>
                                          </Button>
                                       </children>
                                    </VBox>
                                 </content>
                                 <font>
                                    <Font size="18.0" />
                                 </font>
                                 <textFill>
                                    <LinearGradient endX="0.5" endY="1.0" startX="0.4904761904761905">
                                       <stops>
                                          <Stop color="#101ce0" />
                                          <Stop color="#101ce0" offset="0.00749063670411985" />
                                          <Stop color="#652727" offset="1.0" />
                                       </stops>
                                    </LinearGradient>
                                 </textFill>
                              </TitledPane>
                            <TitledPane animated="false" text="帮助">
                                 <content>
                                    <VBox prefHeight="50.0" prefWidth="100.0">
                                    <padding>
                                          <Insets bottom="0" left="0" right="0" top="0" />
                                       </padding>
                                       <children>
                                          <Button fx:id="buttonAboutUs" mnemonicParsing="false" onMousePressed="#onAboutUsClicked" prefHeight="50.0" prefWidth="150.0" text="关于我们">
                                             <font>
                                                <Font size="18.0" />
                                             </font>
                                          </Button>
                                       </children>
                                    </VBox>
                                 </content>
                                 <font>
                                    <Font size="18.0" />
                                 </font>
                                 <textFill>
                                    <LinearGradient endX="0.5" endY="1.0" startX="0.4904761904761905">
                                       <stops>
                                          <Stop color="#101ce0" />
                                          <Stop color="#101ce0" offset="0.00749063670411985" />
                                          <Stop color="#652727" offset="1.0" />
                                       </stops>
                                    </LinearGradient>
                                 </textFill>
                              </TitledPane>
                          </panes>
                        </Accordion>
                     </children>
                  </FlowPane>
                  <AnchorPane fx:id="rightComponentParent" prefHeight="200.0" prefWidth="1050.0" />
               </children>
            </HBox>
            <ImageView fx:id="image" fitHeight="675.0" fitWidth="1200.0" onMousePressed="#imageMove" pickOnBounds="true">
               <image>
                  <Image url="@inStage.jpg" />
               </image>
               <viewport>
                  <Rectangle2D />
               </viewport>
            </ImageView>
         </children>
      </AnchorPane>
   </center>
</BorderPane>
